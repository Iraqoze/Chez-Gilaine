        <activity android:name=".CategoryFoodItemTemplateActivity" />
        <activity android:name=".CartViewItemTemplateActivity" />
        <activity android:name=".ViewCartActivity" />
        <activity android:name=".ProductDetailActivity" />
        <activity android:name=".CategoryItemTemplateActivity" />
        <activity android:name=".HomeMainActivity" />
        <activity android:name=".AddFoodItemActivity" />
        <activity android:name=".AdminHomeActivity" />
        <activity android:name=".FoodItemTemplateActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>