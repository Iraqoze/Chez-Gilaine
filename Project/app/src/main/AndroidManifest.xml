        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".CustomDialogActivity"></activity>
        <activity android:name=".OrderHistoryItemTemplateActivity" />
        <activity android:name=".ViewOrdersActivity" />
